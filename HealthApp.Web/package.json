  "main": "index.html",
  "scripts": {
    "rimraf": "rimraf",
    "tslint": "tslint",
    "typedoc": "typedoc",
    "webpack": "webpack",
    "webpack-dev-server": "webpack-dev-server",
    "webdriver-manager": "webdriver-manager",
    "protractor": "protractor",
    "watch": "webpack --watch",
    "docs": "npm run typedoc -- --options typedoc.json --includeDeclarations typings/index.d.ts --exclude '**/*.spec.ts' ./src/",
    "minify": "npm run minify-vendor && npm run minify-app && npm run minify-polyfills",
    "minify-app": "uglifyjs dist/app.js --screw-ie8 --compress --mangle --output dist/app.min.js",
    "minify-vendor": "uglifyjs dist/vendor.js --screw-ie8 --compress --mangle --output dist/vendor.min.js",
    "minify-polyfills": "uglifyjs dist/polyfills.js --screw-ie8 --compress --mangle --output dist/polyfills.min.js"
  },
